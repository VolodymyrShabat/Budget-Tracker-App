@model IEnumerable<Budget_Tracker_App.Models.Spending>
    <table class="table">

        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Category</td>
                <td>@item.SpendAmount</td>
                <td>
                    <a asp-action="Edit" asp-controller="Budget" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Delete" asp-controller="Budget" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </table>
